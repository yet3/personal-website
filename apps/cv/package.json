{
  "name": "cv",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && rm -rf ./cv-output && mkdir ./cv-output && bun ./scripts/generate-cv-pdf.ts",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "copy:cv": "bun ./scripts/copy-cv-pdf.ts"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^3.0.10",
    "@sveltejs/kit": "^2.50.2",
    "@sveltejs/vite-plugin-svelte": "^6.2.4",
    "browserslist": "^4.25.3",
    "lightningcss": "^1.31.1",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-svelte": "^3.4.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "puppeteer": "^24.37.5",
    "sharp": "^0.34.5",
    "svelte": "^5.51.0",
    "svelte-check": "^4.3.6",
    "typescript": "^5.9.3",
    "unocss": "^66.6.0",
    "vite": "^7.3.1"
  },
  "dependencies": {
    "@repo/lib": "workspace:*",
    "@repo/content": "workspace:*",
    "@unocss/reset": "^66.6.0"
  }
}
