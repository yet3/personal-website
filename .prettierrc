{
	"useTabs": true,
	"singleQuote": false,
	"trailingComma": "none",
	"printWidth": 100,
	"plugins": [
		"prettier-plugin-svelte",
		"prettier-plugin-tailwindcss",
		"@trivago/prettier-plugin-sort-imports"
	],
	"importOrder": [
		"^\\$styles/(.*)$",
		"^\\$moduels/(.*)$",
		"^\\$common/(.*)$",
		"^\\$lib/(.*)$",
		"^\\$types/(.*)$",
		"^\\$assets/(.*)$",
		"^\\$static/(.*)$",
		"^[./]"
	],
  "importOrderSortSpecifiers": true,
  "importOrderSeparation": false,
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte"
			}
		}
	],
	"tailwindStylesheet": "./src/app.css"
}
